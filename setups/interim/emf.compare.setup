<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:mylyn="http://www.eclipse.org/oomph/setup/mylyn/1.0"
    xmlns:predicates="http://www.eclipse.org/oomph/predicates/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.p2="http://www.eclipse.org/oomph/setup/p2/1.0"
    xmlns:setup.targlets="http://www.eclipse.org/oomph/setup/targlets/1.0"
    xmlns:setup.workingsets="http://www.eclipse.org/oomph/setup/workingsets/1.0"
    xmlns:workingsets="http://www.eclipse.org/oomph/workingsets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/setup/mylyn/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Mylyn.ecore http://www.eclipse.org/oomph/predicates/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Predicates.ecore http://www.eclipse.org/oomph/setup/targlets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupTarglets.ecore http://www.eclipse.org/oomph/setup/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupWorkingSets.ecore http://www.eclipse.org/oomph/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/WorkingSets.ecore"
    name="EMF.Compare"
    label="EMF Compare">
  <setupTask
      xsi:type="setup.p2:P2Task">
    <requirements
        id="org.eclipse.pde.api.tools.ee.feature.feature.group"/>
    <requirements
        id="net.sf.eclipsecs.feature.group"/>
    <requirements
        id="edu.umd.cs.findbugs.plugin.eclipse.feature.group"/>
    <requirements
        id="com.mountainminds.eclemma.feature.feature.group"/>
    <requirements
        id="org.eclipse.mylyn.bugzilla_feature.feature.group"/>
    <requirements
        id="org.eclipse.mylyn.gerrit.feature.feature.group"/>
    <requirements
        id="org.eclipse.mylyn.hudson.feature.group"/>
    <requirements
        id="org.eclipse.mylyn.builds.feature.group"/>
    <requirements
        id="org.eclipse.mylyn.wikitext_feature.feature.group"/>
    <requirements
        id="net.jeeeyul.pdetools.feature.feature.group"/>
    <requirements
        id="fr.obeo.releng.targetplatform-feature_3.8.feature.group"/>
    <requirements
        id="org.eclipselabs.emf.loophole.feature.group"/>
    <repositories
        url="${train.url}"/>
    <repositories
        url="http://eclipse-cs.sf.net/update/"/>
    <repositories
        url="http://findbugs.cs.umd.edu/eclipse/"/>
    <repositories
        url="http://update.eclemma.org/"/>
    <repositories
        url="http://download.eclipse.org/mylyn/releases/latest/"/>
    <repositories
        url="https://raw.github.com/jeeeyul/pde-tools/master/net.jeeeyul.pdetools.updatesite/"/>
    <repositories
        url="http://mbarbero.github.com/fr.obeo.releng.targetplatform/p2/3.8"/>
    <repositories
        url="http://mbarbero.github.io/emf-loophole/2.9"/>
  </setupTask>
  <setupTask
      xsi:type="setup:EclipseIniTask"
      option="-Xmx"
      value="1g"
      vm="true"/>
  <setupTask
      xsi:type="setup:ResourceCreationTask"
      excludedTriggers="STARTUP MANUAL"
      content="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xD;&#xA;&lt;section name=&quot;Workbench&quot;>&#xD;&#xA;&#x9;&lt;section name=&quot;org.eclipse.jdt.internal.ui.packageview.PackageExplorerPart&quot;>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;true&quot; key=&quot;group_libraries&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;false&quot; key=&quot;linkWithEditor&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;2&quot; key=&quot;layout&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;2&quot; key=&quot;rootMode&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;&amp;lt;?xml version=&amp;quot;1.0&amp;quot; encoding=&amp;quot;UTF-8&amp;quot;?&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;packageExplorer configured=&amp;quot;true&amp;quot; group_libraries=&amp;quot;1&amp;quot; layout=&amp;quot;2&amp;quot; linkWithEditor=&amp;quot;0&amp;quot; rootMode=&amp;quot;2&amp;quot; sortWorkingSets=&amp;quot;false&amp;quot; workingSetName=&amp;quot;&amp;quot;&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;localWorkingSetManager&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;workingSet editPageId=&amp;quot;org.eclipse.jdt.internal.ui.OthersWorkingSet&amp;quot; factoryID=&amp;quot;org.eclipse.ui.internal.WorkingSetFactory&amp;quot; id=&amp;quot;1382792884467_1&amp;quot; label=&amp;quot;Other Projects&amp;quot; name=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;/localWorkingSetManager&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;activeWorkingSet workingSetName=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;allWorkingSets workingSetName=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;/packageExplorer&amp;gt;&quot; key=&quot;memento&quot;/>&#xD;&#xA;&#x9;&lt;/section>&#xD;&#xA;&lt;/section>&#xD;&#xA;"
      targetURL="${setup.ws.dir|uri}/.metadata/.plugins/org.eclipse.jdt.ui/dialog_settings.xml"
      encoding="UTF-8"/>
  <stream name="master">
    <setupTask
        xsi:type="setup:CompoundTask"
        name="org.eclipse.pde.api.tools">
      <setupTask
          xsi:type="setup:PreferenceTask"
          key="/instance/org.eclipse.pde.api.tools/missing_default_api_profile"
          value="Warning"/>
      <setupTask
          xsi:type="setup:PreferenceTask"
          key="/instance/org.eclipse.pde.api.tools/MISSING_EE_DESCRIPTIONS"
          value="Warning"/>
    </setupTask>
    <setupTask
        xsi:type="git:GitCloneTask"
        location="${setup.branch.dir/git/org.eclipse.emf.compare.git}"
        remoteURI="ssh://git.eclipse.org:29418/emfcompare/org.eclipse.emf.compare.git"
        checkoutBranch="master"/>
    <setupTask
        xsi:type="setup.targlets:TargletTask">
      <targlets
          name="EMF Compare">
        <requirements
            id="org.eclipse.sdk.feature.group"/>
        <requirements
            id="org.eclipse.emf.compare.feature.group"/>
        <requirements
            id="org.eclipse.emf.compare.diagram.ecoretools.feature.group"/>
        <requirements
            id="org.eclipse.emf.compare.diagram.ecoretools.tests"/>
        <requirements
            id="org.eclipse.emf.compare.diagram.gmf.feature.group"/>
        <requirements
            id="org.eclipse.emf.compare.diagram.ide.ecoretools.tests"/>
        <requirements
            id="org.eclipse.emf.compare.diagram.papyrus.feature.group"/>
        <requirements
            id="org.eclipse.emf.compare.diagram.papyrus.tests"/>
        <requirements
            id="org.eclipse.emf.compare.ide.tests"/>
        <requirements
            id="org.eclipse.emf.compare.ide.ui.feature.group"/>
        <requirements
            id="org.eclipse.emf.compare.ide.ui.tests"/>
        <requirements
            id="org.eclipse.emf.compare.rcp.ui.feature.group"/>
        <requirements
            id="org.eclipse.emf.compare.rcp.ui.tests"/>
        <requirements
            id="org.eclipse.emf.compare.tests"/>
        <requirements
            id="org.eclipse.emf.compare.uml2.feature.group"/>
        <requirements
            id="org.eclipse.emf.compare.uml2.tests"/>
        <requirements
            id="org.eclipse.egit.feature.group"/>
        <requirements
            id="org.eclipse.emf.ecoretools.sdk.feature.group"
            versionRange="[1.2.0,2.0.0)"/>
        <requirements
            id="org.eclipse.emf.emfstore.fuzzy.feature.feature.group"/>
        <requirements
            id="org.eclipse.emf.emfstore.modelmutator.feature.feature.group"/>
        <requirements
            id="org.eclipse.emf.sdk.feature.group"/>
        <requirements
            id="org.eclipse.gmf.runtime.sdk.feature.group"/>
        <requirements
            id="org.eclipse.papyrus.sdk.feature.feature.group"/>
        <requirements
            id="org.eclipse.uml2.sdk.feature.group"/>
        <sourceLocators
            rootFolder="${setup.branch.dir/git/org.eclipse.emf.compare.git}"/>
        <repositoryLists
            name="Luna Dependencies">
          <repositories
              url="http://download.eclipse.org/releases/luna"/>
        </repositoryLists>
      </targlets>
    </setupTask>
    <setupTask
        xsi:type="setup.workingsets:WorkingSetTask">
      <workingSets
          name="Core">
        <predicates
            xsi:type="workingsets:ExclusionPredicate"
            excludedWorkingSets="//@streams[name='master']/@setupTasks.3/@workingSets.6 //@streams[name='master']/@setupTasks.3/@workingSets.1 //@streams[name='master']/@setupTasks.3/@workingSets.2 //@streams[name='master']/@setupTasks.3/@workingSets.3 //@streams[name='master']/@setupTasks.3/@workingSets.4 //@streams[name='master']/@setupTasks.3/@workingSets.7"/>
      </workingSets>
      <workingSets
          name="RCP">
        <predicates
            xsi:type="predicates:AndPredicate">
          <operands
              xsi:type="predicates:NamePredicate"
              pattern=".*compare.rcp.*"/>
          <operands
              xsi:type="workingsets:ExclusionPredicate"
              excludedWorkingSets="//@streams[name='master']/@setupTasks.3/@workingSets.7"/>
        </predicates>
      </workingSets>
      <workingSets
          name="IDE">
        <predicates
            xsi:type="predicates:AndPredicate">
          <operands
              xsi:type="predicates:NamePredicate"
              pattern=".*compare.ide.*"/>
          <operands
              xsi:type="workingsets:ExclusionPredicate"
              excludedWorkingSets="//@streams[name='master']/@setupTasks.3/@workingSets.7"/>
        </predicates>
      </workingSets>
      <workingSets
          name="UML">
        <predicates
            xsi:type="predicates:AndPredicate">
          <operands
              xsi:type="predicates:NamePredicate"
              pattern=".*uml.*"/>
          <operands
              xsi:type="workingsets:ExclusionPredicate"
              excludedWorkingSets="//@streams[name='master']/@setupTasks.3/@workingSets.7"/>
        </predicates>
      </workingSets>
      <workingSets
          name="Diagram">
        <predicates
            xsi:type="predicates:AndPredicate">
          <operands
              xsi:type="predicates:NamePredicate"
              pattern=".*diagram.*"/>
          <operands
              xsi:type="workingsets:ExclusionPredicate"
              excludedWorkingSets="//@streams[name='master']/@setupTasks.3/@workingSets.7"/>
        </predicates>
      </workingSets>
      <workingSets
          name="Tests">
        <predicates
            xsi:type="predicates:FilePredicate"
            filePattern="META-INF/MANIFEST.MF"
            contentPattern="org\.junit"/>
        <predicates
            xsi:type="predicates:NamePredicate"
            pattern=".*tests.*"/>
      </workingSets>
      <workingSets
          name="Packaging">
        <predicates
            xsi:type="predicates:NaturePredicate"
            nature="org.eclipse.pde.UpdateSiteNature"/>
        <predicates
            xsi:type="predicates:NamePredicate"
            pattern=".*-parent"/>
        <predicates
            xsi:type="predicates:NamePredicate"
            pattern=".*doc"/>
      </workingSets>
      <workingSets
          name="Features">
        <predicates
            xsi:type="predicates:NaturePredicate"
            nature="org.eclipse.pde.FeatureNature"/>
      </workingSets>
    </setupTask>
    <setupTask
        xsi:type="mylyn:MylynQueriesTask"
        repositoryURL="https://bugs.eclipse.org/bugs">
      <queries
          summary="Open bugs"
          url="https://bugs.eclipse.org/bugs/buglist.cgi?short_desc_type=allwordssubstr&amp;short_desc=&amp;product=EMFCompare&amp;long_desc_type=allwordssubstr&amp;long_desc=&amp;bug_status=UNCONFIRMED&amp;bug_status=NEW&amp;bug_status=ASSIGNED&amp;bug_status=REOPENED&amp;status_whiteboard_type=allwordssubstr&amp;status_whiteboard=&amp;order=Importance"/>
    </setupTask>
    <setupTask
        xsi:type="mylyn:MylynQueriesTask"
        connectorKind="org.eclipse.mylyn.gerrit"
        repositoryURL="https://git.eclipse.org/r">
      <queries
          summary="Open reviews">
        <attributes
            key="gerrit query type"
            value="open changes by project"/>
        <attributes
            key="gerrit query project"
            value="emfcompare/org.eclipse.emf.compare"/>
        <attributes
            key="gerrit query string"
            value=""/>
      </queries>
    </setupTask>
    <setupTask
        xsi:type="mylyn:MylynBuildsTask"
        serverURL="https://hudson.eclipse.org/emfcompare/">
      <buildPlans
          name="gerrit-allbranches"/>
      <buildPlans
          name="performance-tests"/>
      <buildPlans
          name="master-nightly"/>
    </setupTask>
  </stream>
</setup:Project>
