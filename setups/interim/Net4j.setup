    </setupTask>
    <stream
        name="master"
        label="Master">
      <setupTask
          xsi:type="setup:VariableTask"
          name="bugzilla.version"
          value="4.4"/>
      <setupTask
          xsi:type="setup:VariableTask"
          name="baseline.version"
          value="4.3"/>
    </stream>
    <stream
        name="streams/4.3-maintenance"
        label="4.3 Maintenance">
      <setupTask
          xsi:type="setup:VariableTask"
          name="bugzilla.version"
          value="4.3"/>
      <setupTask
          xsi:type="setup:VariableTask"
          name="baseline.version"
          value="4.3"/>
    </stream>
    <description>A collection of core and UI utilities.</description>
  </project>
  <description>A container for Net4j Signalling Platform, Net4j DB Framework and Net4j Utilities.</description>
</setup:Project>
