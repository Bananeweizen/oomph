    <requirement
        id="org.eclipse.pde.api.tools.ee.feature.feature.group"/>
    <requirement
        id="org.eclipse.net4j.util.ui.feature.group"/>
    <requirement
        id="org.eclipse.mylyn.ide_feature.feature.group"/>
    <requirement
        id="org.eclipse.mylyn.java_feature.feature.group"/>
    <requirement
        id="org.eclipse.mylyn.pde_feature.feature.group"/>
    <requirement
        id="org.eclipse.mylyn.team_feature.feature.group"/>
    <requirement
        id="org.eclipse.mylyn.gerrit.feature.feature.group"/>
    <requirement
        id="org.eclipse.mylyn_feature.feature.group"/>
    <requirement
        id="org.eclipse.mylyn.bugzilla_feature.feature.group"/>
    <requirement
        id="org.eclipse.mylyn.git.feature.group"/>
    <requirement
        id="org.eclipse.egit.feature.group"/>
    <requirement
        id="org.eclipse.egit.mylyn.feature.group"/>
    <requirement
        id="org.eclipse.oomph.projectcopy.feature.group"/>
    <requirement
        id="org.eclipse.oomph.version.feature.group"/>
    <repository
        url="http://download.eclipse.org/mylyn/snapshots/weekly"/>
    <repository
        url="http://download.eclipse.org/egit/updates-nightly"/>
    <repository
        url="http://download.eclipse.org/modeling/emf/cdo/drops/R20140610-0212"/>
    <repository
        url="http://download.eclipse.org/oomph/updates"/>
  </setupTask>
  <setupTask
      xsi:type="setup:ResourceCreationTask"
      excludedTriggers="STARTUP MANUAL"
      content="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xD;&#xA;&lt;section name=&quot;Workbench&quot;>&#xD;&#xA;&#x9;&lt;section name=&quot;org.eclipse.jdt.internal.ui.packageview.PackageExplorerPart&quot;>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;true&quot; key=&quot;group_libraries&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;false&quot; key=&quot;linkWithEditor&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;2&quot; key=&quot;layout&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;2&quot; key=&quot;rootMode&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;&amp;lt;?xml version=&amp;quot;1.0&amp;quot; encoding=&amp;quot;UTF-8&amp;quot;?&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;packageExplorer configured=&amp;quot;true&amp;quot; group_libraries=&amp;quot;1&amp;quot; layout=&amp;quot;2&amp;quot; linkWithEditor=&amp;quot;0&amp;quot; rootMode=&amp;quot;2&amp;quot; sortWorkingSets=&amp;quot;false&amp;quot; workingSetName=&amp;quot;&amp;quot;&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;localWorkingSetManager&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;workingSet editPageId=&amp;quot;org.eclipse.jdt.internal.ui.OthersWorkingSet&amp;quot; factoryID=&amp;quot;org.eclipse.ui.internal.WorkingSetFactory&amp;quot; id=&amp;quot;1382792884467_1&amp;quot; label=&amp;quot;Other Projects&amp;quot; name=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;/localWorkingSetManager&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;activeWorkingSet workingSetName=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;allWorkingSets workingSetName=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;/packageExplorer&amp;gt;&quot; key=&quot;memento&quot;/>&#xD;&#xA;&#x9;&lt;/section>&#xD;&#xA;&lt;/section>&#xD;&#xA;"
      targetURL="${workspace.location|uri}/.metadata/.plugins/org.eclipse.jdt.ui/dialog_settings.xml"
      encoding="UTF-8"/>
  <setupTask
      xsi:type="setup:PreferenceTask"
      key="instance/org.eclipse.jdt.launching/org.eclipse.jdt.launching.PREF_STRICTLY_COMPATIBLE_JRE_NOT_AVAILABLE"
      value="ignore"/>
  <setupTask
      xsi:type="setup:PreferenceTask"
      key="instance/org.eclipse.jdt.core/org.eclipse.jdt.core.compiler.problem.unclosedCloseable"
      value="ignore"/>
  <setupTask
      xsi:type="setup:PreferenceTask"
      key="instance/org.eclipse.pde/compilers.f.unresolved-features"
      value="2"/>
  <setupTask
      xsi:type="pde:APIBaselineTask"
      id="cdo.baseline"
      name="CDO"
      version="${baseline.version}"
      remoteURI="http://sourceforge.net/projects/net4j/files/Baselines/emf-cdo-${baseline.version}-baseline.zip/download"/>
  <setupTask
      xsi:type="git:GitCloneTask"
      id="cdo.git.clone"
      remoteURI="cdo/cdo">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="inherit">
        <value>eclipse.git.gerrit.remoteURIs</value>
      </detail>
      <detail
          key="label">
        <value>Net4j Git or Gerrit Repository</value>
      </detail>
      <detail
          key="target">
        <value>remoteURI</value>
      </detail>
    </annotation>
    <description>CDO</description>
  </setupTask>
  <setupTask
      xsi:type="mylyn:MylynQueriesTask"
      repositoryURL="https://bugs.eclipse.org/bugs"
      userID="${bugzilla.id}"
      password="${eclipse.user.password}">
    <query summary="Net4j Signalling Platform ${bugzilla.version} Bugs"
        url="https://bugs.eclipse.org/bugs/buglist.cgi?product=EMF&amp;${bugzilla.components}&amp;bug_status=UNCONFIRMED&amp;bug_status=NEW&amp;bug_status=ASSIGNED&amp;bug_status=REOPENED&amp;version=${bugzilla.version}&amp;bug_severity=blocker&amp;bug_severity=critical&amp;bug_severity=major&amp;bug_severity=normal&amp;bug_severity=minor&amp;bug_severity=trivial"/>
    <query summary="Net4j Signalling Platform ${bugzilla.version} Features"
        url="https://bugs.eclipse.org/bugs/buglist.cgi?product=EMF&amp;${bugzilla.components}&amp;bug_status=UNCONFIRMED&amp;bug_status=NEW&amp;bug_status=ASSIGNED&amp;bug_status=REOPENED&amp;version=${bugzilla.version}&amp;bug_severity=enhancement"/>
    <query summary="Net4j Signalling Platform ${bugzilla.version} Resolved"
        url="https://bugs.eclipse.org/bugs/buglist.cgi?product=EMF&amp;${bugzilla.components}&amp;bug_status=RESOLVED&amp;resolution=FIXED&amp;version=${bugzilla.version}"/>
    <query summary="Net4j Signalling Platform ${bugzilla.version} Unspecified Version"
        url="https://bugs.eclipse.org/bugs/buglist.cgi?product=EMF&amp;${bugzilla.components}&amp;version=unspecified"/>
  </setupTask>
  <project name="core"
      label="Signalling Platform">
    <setupTask
        xsi:type="setup:VariableTask"
        name="bugzilla.components"
        value="component=cdo.net4j&amp;component=cdo.net4j.ui"/>
    <setupTask
        xsi:type="setup.targlets:TargletTask">
      <targlet
          name="Net4j Signalling Platform">
        <requirement
            id="org.eclipse.net4.feature.group"/>
        <requirement
            id="org.eclipse.net4.defs.feature.group"/>
        <requirement
            id="org.eclipse.net4.doc.feature.group"/>
        <requirement
            id="org.eclipse.net4.examples.feature.group"/>
        <requirement
            id="org.eclipse.net4.examples.installer.feature.group"/>
        <requirement
            id="org.eclipse.net4.sdk.feature.group"/>
        <requirement
            id="org.eclipse.net4.tests.feature.group"/>
        <requirement
            id="org.eclipse.net4.ui.feature.group"/>
        <sourceLocator
            rootFolder="${cdo.git.clone.location}"/>
        <repositoryList
            name="Dependencies">
          <repository
              url="http://download.eclipse.org/eclipse/updates/4.4/R-4.4-201406061215"/>
        </repositoryList>
      </targlet>
    </setupTask>
    <setupTask
        xsi:type="setup.workingsets:WorkingSetTask">
      <workingSet
          name="Net4j Signalling Platform"
          id="Net4j Signalling Platform">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.net4j"/>
          <operand
              xsi:type="predicates:NamePredicate"
              pattern="org\.eclipse\.net4j.*"/>
          <operand
              xsi:type="workingsets:ExclusionPredicate"
              excludedWorkingSet="//@projects[name='core']/@setupTasks.2/@workingSets.1"/>
          <operand
              xsi:type="predicates:NotPredicate">
            <operand
                xsi:type="predicates:NamePredicate"
                pattern=".*\.db.*|.*\.util.*"/>
          </operand>
        </predicate>
      </workingSet>
      <workingSet
          name="Net4j Signalling Platform Examples"
          id="Net4j Signalling Platform Examples">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.net4j"/>
          <operand
              xsi:type="predicates:OrPredicate">
            <operand
                xsi:type="predicates:NamePredicate"
                pattern="org\.eclipse\.net4j\.examples.*"/>
            <operand
                xsi:type="predicates:NamePredicate"
                pattern="org\.eclipse\.net4j\.buddies.*"/>
            <operand
                xsi:type="predicates:NamePredicate"
                pattern="org\.eclipse\.net4j\.jms.*"/>
          </operand>
        </predicate>
      </workingSet>
    </setupTask>
    <stream
        name="master"
        label="Master">
      <setupTask
          xsi:type="setup:VariableTask"
          name="bugzilla.version"
          value="4.4"/>
      <setupTask
          xsi:type="setup:VariableTask"
          name="baseline.version"
          value="4.3"/>
    </stream>
    <stream
        name="streams/4.3-maintenance"
        label="4.3 Maintenance">
      <setupTask
          xsi:type="setup:VariableTask"
          name="bugzilla.version"
          value="4.3"/>
      <setupTask
          xsi:type="setup:VariableTask"
          name="baseline.version"
          value="4.3"/>
    </stream>
    <description>A fast and asynchronous client/server communications layer that separates transports from protocols.</description>
  </project>
  <project name="db"
      label="DB Framework">
    <setupTask
        xsi:type="setup:VariableTask"
        name="bugzilla.components"
        value="component=cdo.net4j.db"/>
    <setupTask
        xsi:type="setup.targlets:TargletTask">
      <targlet
          name="Net4j DB Framework">
        <requirement
            id="org.eclipse.net4j.db.feature.group"/>
        <requirement
            id="org.eclipse.net4j.db.db2.feature.group"/>
        <requirement
            id="org.eclipse.net4j.db.derby.feature.group"/>
        <requirement
            id="org.eclipse.net4j.db.doc.feature.group"/>
        <requirement
            id="org.eclipse.net4j.db.h2.feature.group"/>
        <requirement
            id="org.eclipse.net4j.db.hsqldb.feature.group"/>
        <requirement
            id="org.eclipse.net4j.db.mysql.feature.group"/>
        <requirement
            id="org.eclipse.net4j.db.oracle.feature.group"/>
        <requirement
            id="org.eclipse.net4j.db.postgresql.feature.group"/>
        <sourceLocator
            rootFolder="${cdo.git.clone.location}"/>
        <repositoryList
            name="Dependencies">
          <repository
              url="http://download.eclipse.org/eclipse/updates/4.4/R-4.4-201406061215"/>
          <repository
              url="http://download.eclipse.org/modeling/emf/cdo/orbit/latest-R"/>
        </repositoryList>
      </targlet>
    </setupTask>
    <setupTask
        xsi:type="setup.workingsets:WorkingSetTask">
      <workingSet
          name="Net4j DB Framework"
          id="Net4j DB Framework">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.net4j.db"/>
          <operand
              xsi:type="predicates:NamePredicate"
              pattern="org\.eclipse\.net4j\.db.*"/>
        </predicate>
      </workingSet>
    </setupTask>
    <stream
        name="master"
        label="Master">
      <setupTask
          xsi:type="setup:VariableTask"
          name="bugzilla.version"
          value="4.4"/>
      <setupTask
          xsi:type="setup:VariableTask"
          name="baseline.version"
          value="4.3"/>
      <setupTask
          xsi:type="setup.targlets:TargletTask">
        <targlet
            name="Net4j DB Framework">
          <repositoryList
              name="Dependencies">
            <repository
                url="http://download.eclipse.org/eclipse/updates/4.4/R-4.4-201406061215"/>
            <repository
                url="http://download.eclipse.org/modeling/emf/cdo/orbit/latest-R"/>
          </repositoryList>
        </targlet>
      </setupTask>
    </stream>
    <stream
        name="streams/4.3-maintenance"
        label="4.3 Maintenance">
      <setupTask
          xsi:type="setup:VariableTask"
          name="bugzilla.version"
          value="4.3"/>
      <setupTask
          xsi:type="setup:VariableTask"
          name="baseline.version"
          value="4.3"/>
    </stream>
    <description>A JDBC framework that provides simple and flexible abstractions for vendor-specific DDL.</description>
  </project>
  <project name="util"
