    <setupTask
        xsi:type="setup:VariableTask"
        name="bugzilla.components"
        value="component=cdo.net4j.util"/>
    <setupTask
        xsi:type="setup.targlets:TargletTask">
      <targlet
          name="Net4j Utilities">
        <requirement
            id="org.eclipse.net4j.util.feature.group"/>
        <requirement
            id="org.eclipse.net4j.util.doc.feature.group"/>
        <requirement
            id="org.eclipse.net4j.util.ui.feature.group"/>
        <requirement
            id="org.eclipse.sdk.feature.group"/>
        <sourceLocator
            rootFolder="${cdo.git.clone.location}"/>
        <repositoryList
            name="Dependencies">
          <repository
              url="http://download.eclipse.org/eclipse/updates/4.4/R-4.4-201406061215"/>
        </repositoryList>
      </targlet>
    </setupTask>
    <setupTask
        xsi:type="setup.workingsets:WorkingSetTask">
      <workingSet
          name="Net4j Utilities"
          id="Net4j Utilities">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:RepositoryPredicate"
              project="org.eclipse.net4j.util"/>
          <operand
              xsi:type="predicates:OrPredicate">
            <operand
                xsi:type="predicates:NamePredicate"
                pattern="org\.eclipse\.net4j\.util.*"/>
            <operand
                xsi:type="predicates:NamePredicate"
                pattern="org\.eclipse\.net4j\.debug.*"/>
            <operand
                xsi:type="predicates:NamePredicate"
                pattern="org\.eclipse\.net4j\.ui\.shared"/>
          </operand>
        </predicate>
